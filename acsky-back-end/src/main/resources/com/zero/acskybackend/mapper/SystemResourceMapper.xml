<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zero.acskybackend.repo.mapper.SystemResourceMapper">

    <select id="querySystemResourceList" resultType="com.zero.acskybackend.model.po.SystemResource">
        select
        id, name, url, identity, request_method,
        create_date, update_date
        from
        system_resource
    </select>

</mapper>